.lazyer-button-icon-search-circle,.lazyer-button-icon-edit-circle,.lazyer-button-icon-check-circle,
.lazyer-button-icon-message-circle,.lazyer-button-icon-star-off-circle,.lazyer-button-icon-delete-circle{
