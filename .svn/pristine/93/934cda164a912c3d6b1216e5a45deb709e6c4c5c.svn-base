<script src="lazyerJs.js"></script>
<script>
    lazyerJs.init(function () {
        let app = lazyerJs;
        let $ = app.$;
        app.requseModule(['waterfall'],function () {
            let page = app.waterfall;
            page.init('waterfall');
            //or
            //page.setDefaultDivId('waterfall');
            page.setWaterfall();
            let html = $('#waterfall').html();
            page.setPageIsOver(function () {
                $('#waterfall').append(html)
            })
        })
    });

</script>
