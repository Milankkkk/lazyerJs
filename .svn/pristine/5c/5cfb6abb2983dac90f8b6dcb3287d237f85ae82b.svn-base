/*index  首页样式*/
.lazyer-index-header{
    height: 80px;
    display: flex;
    align-items: center;
    justify-content: space-between;
    background: white;
    padding: 0;
    border-bottom: 1px solid #dcdfe6;
}
.lazyer-index-header-left{
    font-size: 38px;
    color:#409eff ;
}
.lazyer-index-header ul {
    display: flex;
    align-items: center;
}
.lazyer-index-header ul li a{
    text-decoration: none;
    color: #1989fa;
    opacity: .5;
    margin: 0 22px;
    display: inline-block;
    height: 80px;
    line-height: 80px;
    border-bottom: 2px solid  transparent;
    cursor: pointer;
}
.lazyer-index-header ul li a:hover{
    opacity: 1;
}
.lazyer-index-header ul li .aCur{
    border-bottom: 2px solid  #409eff;
    opacity: 1;
}
.lazyer-index-aside,.lazyer-index-main{
    background: white;
}
.lazyer-index-aside{
    width: 240px;
    height: calc(100vh - 80px);
    display: block;
    padding-right: 30px;
}
.lazyer-index-aside-content{
    overflow-y: scroll;
    width: 100%;
    height: 100%;
    padding-top: 50px;
}
.lazyer-index-aside-content>div{
    margin-bottom: 20px;
}
.lazyer-index-aside-content::-webkit-scrollbar{
    width: 5px;
    background-color: transparent;
}
.lazyer-index-aside-content::-webkit-scrollbar-track{
    background-color:transparent;
}
.lazyer-index-aside-content::-webkit-scrollbar-thumb{
    border-radius: 10px;
    -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.2);
    background-color: rgba(144,147,153,.1);
}
.lazyer-index-aside-title{
    font-size: 16px;
    line-height: 40px;
    height: 40px;
    font-weight: bold;
}
.lazyer-index-aside-stitle{
    font-size: 12px;
    color: #999;
    height: 26px;
    line-height: 26px;
    margin-top: 10px;
}
.lazyer-index-aside-item a{
    display: block;
    height: 40px;
    color: #444;
    line-height: 40px;
    font-size: 14px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    font-weight: 400;
}
.lazyer-index-aside-item a:hover{
    color: #409eff;
}
.lazyer-index-aside-item .aCur{
    color: #409eff;
}
.lazyer-index-main{
    height: auto;
    display: block;
}
.lazyer-index-main iframe{
    width: 100%;
    height: 100%;
}
::-webkit-scrollbar{
    width: 5px;
    background-color: transparent;
}
::-webkit-scrollbar-track{
    background-color:transparent;
}
::-webkit-scrollbar-thumb{
    border-radius: 10px;
    -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.2);
    background-color: rgba(144,147,153,.1);
}
.lazyer-index-table{
    width: 100%;
    border-collapse: collapse;
}
.lazyer-index-table th,.lazyer-index-table td{
    border-bottom: 1px solid #dcdfe6;
    text-align: center;
    padding: 15px;
    max-width: 250px;
}
@media screen and (min-width: 1140px) {
    .lazyer-index-container{
        width: 1140px;
        margin: 0 auto;
    }
}



