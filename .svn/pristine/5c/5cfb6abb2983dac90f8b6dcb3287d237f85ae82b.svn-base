*{
    margin: 0;
    padding: 0;
    border: 0;
    box-sizing: border-box;
    color: #333;
    font-family: "Helvetica Neue",Helvetica,"PingFang SC","Hiragino Sans GB","Microsoft YaHei","微软雅黑",Arial,SimSun,sans-serif;
}
button,input{
    outline: none;
}
li{
    list-style: none;
}
a{
    text-decoration: none;
}
h2{
    font-weight: 400;
    color: #1f2f3d;
    font-size: 28px;
    margin-top: 50px;
}
p{
    font-size: 14px;
    color: #5e6d82;
    margin-top: 10px;
}
h3{
    margin: 55px 0 10px;
    font-weight: 400;
    color: #1f2f3d;
    font-size: 22px;
}
.tip{
    padding: 8px 16px;
    background-color: #ecf8ff;
    border-radius: 4px;
    border-left: 5px solid #50bfff;
    margin: 20px 0;
}
.tip p{
    margin: 14px 0;
}
.lazyer-content-container{
    border: 1px solid #ebebeb;
    border-radius: 3px;
    padding: 20px 20px 0 20px;
    margin-top: 20px;
}
.lazyer-class-box{
    padding: 10px 0 0 20px;
    font-size: 14px;
    color: #5e6d82;
    line-height: 2em;
}
.lazyer-class-box li{
    list-style: disc !important;
}

/*layout 栅格布局样式*/
.lazyer-layout-Box{
    margin-top: 35px;
}
.lazyer-layout-Box p{
    margin: 0 0 5px 0;
}
.lazyer-row{
    width: 100%;
    display: -ms-flexbox;
    -ms-flex-wrap: wrap;
    display: flex;
    flex-wrap: wrap;
    margin-bottom: 20px;
}
.lazyer-row>div{
    height:35px;
    line-height: 35px;
    text-align: center;
}
.lazyer-row>div:nth-of-type(2n){
    background: #e5e9f2;
}
.lazyer-row>div:nth-of-type(2n+1){
    background: #d3dce6;
}
.lazyer-col{
    -ms-flex-preferred-size: 0;
    -ms-flex-positive: 1;
    flex-basis: 0;
    flex-grow: 1;
    max-width: 100%;
}
.lazyer-col-sm-1,.lazyer-col-md-1,.lazyer-col-lg-1,.lazyer-col-xl-1{
    width: calc(100% / 12);
}
.lazyer-col-sm-2,.lazyer-col-md-2,.lazyer-col-lg-2,.lazyer-col-xl-2{
    width:calc(100% / 6);
}
.lazyer-col-sm-3,.lazyer-col-md-3,.lazyer-col-lg-3,.lazyer-col-xl-3{
    width:calc(100% / 4);
}
.lazyer-col-sm-4,.lazyer-col-md-4,.lazyer-col-lg-4,.lazyer-col-xl-4{
    width:calc(100% / 3);
}
.lazyer-col-sm-5,.lazyer-col-md-5,.lazyer-col-lg-5,.lazyer-col-xl-5{
    width:calc(100% / 12 * 5);
}
.lazyer-col-sm-6,.lazyer-col-md-6,.lazyer-col-lg-6,.lazyer-col-xl-6{
    width:calc(100% / 2);
}
.lazyer-col-sm-7,.lazyer-col-md-7,.lazyer-col-lg-7,.lazyer-col-xl-7{
    width:calc(100% / 12 * 7);
}
.lazyer-col-sm-8,.lazyer-col-md-8,.lazyer-col-lg-8,.lazyer-col-xl-8{
    width:calc(100% / 12 * 8);
}
.lazyer-col-sm-9,.lazyer-col-md-9,.lazyer-col-lg-9,.lazyer-col-xl-9{
    width:calc(100% / 12 * 9);
}
.lazyer-col-sm-10,.lazyer-col-md-10,.lazyer-col-lg-10,.lazyer-col-xl-10{
    width:calc(100% / 12 * 10);
}
.lazyer-col-sm-11,.lazyer-col-md-11,.lazyer-col-lg-11,.lazyer-col-xl-11{
    width:calc(100% / 12 * 11);
}
.lazyer-col-sm-12,.lazyer-col-md-12,.lazyer-col-lg-12,.lazyer-col-xl-12{
    width:100%;
}

/*container  布局容器样式*/
.lazyer-container{
    width: 100%;
    display: flex;
    flex-direction: row;
    flex: 1;
}
.lazyer-container.is-vertical {
    flex-direction: column;
}
.lazyer-header,.lazyer-footer{
    width: 100%;
    height: 60px;
    background: #B3C0D1;
    padding: 0 20px;
}
.lazyer-header,.lazyer-main,.lazyer-footer,.lazyer-aside{
    display: flex;
    justify-content: center;
    align-items: center;
}
.lazyer-main{
    flex: 1;
    flex-basis:auto;
    background: #E9EEF3;
    height: 160px;
    padding-bottom: 5px;
}
.lazyer-aside{
    background-color: #d3dce6;
}

/*color 色彩样式*/
.lazyer-color-box{
    display: flex;
    flex-direction: column;
    height: 114px;
    border-radius: 5px;
    overflow: hidden;
    margin-top: 15px;
}
.lazyer-color-box-top{
    height: 74px;
    padding: 20px 0 0 20px;
}
.lazyer-color-box-bot{
    height: calc(114px - 74px);
    display: flex;
    align-items: center;
}
.lazyer-color-box-bot>div{
    flex: 1;
    height: 100%;
}
.lazyer-color-colorName{
    font-size: 14px;
    color: white;
}
.lazyer-color-code{
    font-size: 12px;
    opacity: .69;
    line-height: 24px;
    color: white;
}
.lazyer-color-essential-top{
    background:#409EFF ;
}
.lazyer-color-blue1{
    background:rgb(83, 168, 255);
}
.lazyer-color-blue2{
    background:rgb(102, 177, 255);
}
.lazyer-color-blue3{
    background:rgb(121, 187, 255);
}
.lazyer-color-blue4{
    background:rgb(140, 197, 255);
}
.lazyer-color-blue5{
    background:rgb(160, 207, 255);
}
.lazyer-color-blue6{
    background:rgb(179, 216, 255);
}
.lazyer-color-blue7{
    background:rgb(198, 226, 255);
}
.lazyer-color-blue8{
    background:rgb(217, 236, 255);
}
.lazyer-color-blue9{
    background:rgb(236, 245, 255);
}
.lazyer-color-assist{
    width: 100%;
    display: flex;
    align-items: center;
}
.lazyer-color-assist>div{
    margin-right: 10px;
}
.lazyer-color-assist>div:last-of-type{
    margin-right: 0;
}
.lazyer-color-success-top{
    background:rgb(103, 194, 58) ;
}
.lazyer-color-success1{
    background: rgb(225, 243, 216);
}
.lazyer-color-success2{
    background:rgb(240, 249, 235) ;
}
.lazyer-color-Warning-top{
    background:rgb(230, 162, 60) ;
}
.lazyer-color-Warning1{
    background:rgb(250, 236, 216);
}
.lazyer-color-Warning2{
    background:rgb(253, 246, 236) ;
}
.lazyer-color-Danger-top{
    background:rgb(245, 108, 108) ;
}
.lazyer-color-Danger1{
    background:rgb(253, 226, 226);
}
.lazyer-color-Danger2{
    background:rgb(254, 240, 240) ;
}
.lazyer-color-Info-top{
    background:rgb(144, 147, 153) ;
}
.lazyer-color-Info1{
    background:rgb(233, 233, 235);
}
.lazyer-color-Info2{
    background:rgb(244, 244, 245);
}
.lazyer-color-neutral{
    display: flex;
}
.lazyer-color-neutral>div{
    margin-right: 10px;
}
.lazyer-color-neutral>div:last-of-type{
    margin-right: 0;
}
.lazyer-color-neutral>div>div{
    height: 74px;
    border-radius: 5px;
    padding: 15px 0 0 20px;
    margin: 15px 0;
}
.lazyer-color-text1{
    background:#303133 ;
}
.lazyer-color-text2{
    background:#606266;
}
.lazyer-color-text3{
    background:#909399;
}
.lazyer-color-text4{
    background:#C0C4CC;
}
.lazyer-color-neutral-border .lazyer-color-colorName,
.lazyer-color-neutral-border .lazyer-color-code{
    color: #303133;
}
.lazyer-color-border1{
    background:#DCDFE6;
}
.lazyer-color-border2{
    background:#E4E7ED;
}
.lazyer-color-border3{
    background:#EBEEF5;
}
.lazyer-color-border4{
    background:#F2F6FC;
}
.lazyer-color-basics1{
    background:#000000;
}
.lazyer-color-basics2{
    background:#FFFFFF;
    border: 1px solid rgb(238, 238, 238);
}
.lazyer-color-basics2 .lazyer-color-colorName,
.lazyer-color-basics2 .lazyer-color-code{
    color: rgb(48, 49, 51);
}
.lazyer-color-basics3{
    background:transparent;
    border: 1px solid #fcc3c3;
    position: relative;
}
.lazyer-color-basics3 .lazyer-color-colorName,
.lazyer-color-basics3 .lazyer-color-code{
    color: #303133;
}
.lazyer-color-diagonal{
    position: absolute;
    top: 35px;
    left: -5px;
    width: 105%;
    height: 1px;
    background: #fcc3c3;
    transform: rotate(160deg);
    z-index: -2;
}

/*typography 字体*/
.lazyer-typography-table th,.lazyer-typography-table td{
    text-align: left !important;
    color: #606266;
}
.lazyer-typography-tr1{
    font-size: 12px;
}
.lazyer-typography-tr2{
    font-size: 13px;
}
.lazyer-typography-tr3{
    font-size: 14px;
}
.lazyer-typography-tr4{
    font-size: 16px;
}
.lazyer-typography-tr5{
    font-size: 18px;
}
.lazyer-typography-tr6{
    font-size: 20px;
}
.lazyer-typography-lineheight{
    margin-top: 30px;
    display: flex;
    align-items: center;
}
.lazyer-typography-lineheight-box>div{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-left: 90px;
    width: 280px;
    font-size: 13px;
    color: #666;
    height: 20px;
    line-height: 20px;
}
.lazyer-typography-code{
    line-height: 1.8;
    font-family: Menlo,Monaco,Consolas,Courier,monospace;
    font-size: 12px;
    padding: 18px 24px;
    background-color: #fafafa;
    border: 1px solid #eaeefb;
    margin-bottom: 25px;
    border-radius: 4px;
}

/*border 边框*/
.lazyer-border-table th,.lazyer-border-table td{
    text-align: left !important;
}
.lazyer-border-table td div{
    width: 570px;
    height: 0;
}
.lazyer-border-td-div1{
    border-top: 1px solid #eee;
}
.lazyer-border-td-div2{
    border-top: 2px dashed #eee;
}
.lazyer-border-radius{
    display: flex;
    align-items: center;
}
.lazyer-border-radius>div{
    margin-right: 80px;
}
.lazyer-border-radius>div>div{
    margin: 10px 0;
}
.lazyer-border-radius>div>div:nth-of-type(1){
    color: #666;
    font-size: 18px;
}
.lazyer-border-radius>div>div:nth-of-type(2){
    color: #333;
    font-size: 16px;
}
.lazyer-border-radius>div>div:nth-of-type(3){
    height: 60px;
    width: 100%;
    border: 1px solid #d7dae2;
}
.lazyer-border-radius0{
    border-radius: 0;
}
.lazyer-border-radius2{
     border-radius: 2px;
}
.lazyer-border-radius4{
    border-radius: 4px;
}
.lazyer-border-radius30{
    border-radius: 30px;
}
.lazyer-border-box-shadow1,
.lazyer-border-box-shadow2{
    height: 100px;
    width: 420px;
    margin: 15px 0 0 10px;
}
.lazyer-border-box-shadow1{
    box-shadow: 0 2px 4px rgba(0, 0, 0, .12), 0 0 6px rgba(0, 0, 0, .04);
}
.lazyer-border-box-shadow2{
    box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1);
}



/*button  按钮*/
.lazyer-button-basics-button{
    display: flex;
    align-items: center;
    margin-bottom: 20px;
}
.lazyer-button,.lazyer-button-primary,.lazyer-button-success,
.lazyer-button-info,.lazyer-button-warning,.lazyer-button-danger,
.lazyer-button-plain,.lazyer-button-primary-plain,.lazyer-button-success-plain,
.lazyer-button-info-plain,.lazyer-button-warning-plain,.lazyer-button-danger-plain,
.lazyer-button-round,.lazyer-button-primary-round,.lazyer-button-success-round,
.lazyer-button-info-round,.lazyer-button-warning-round,.lazyer-button-danger-round{
    padding: 10px 20px;
    cursor: pointer;
    background:#fff;
    border: 1px solid #dcdfe6;
    white-space: nowrap;
    outline: none;
    font-size: 14px;
    border-radius: 4px;
    margin-right:15px;
    color: white;
    display: flex;
    align-items: center;
}
.lazyer-button,.lazyer-button-plain,.lazyer-button-round{
    color: #606266;
}
.lazyer-button:hover,.lazyer-button-round:hover,.lazyer-button-icon-search-circle:hover{
